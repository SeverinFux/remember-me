{
  "expo": {
    "name": "remember-me",
    "slug": "remember-me",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "userInterfaceStyle": "light",
    "scheme": "remember-me",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#b4e8dd"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#b4e8dd"
      },
      "package": "com.uek.rememberme"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-media-library",
        {
          "isAccessMediaLocationRequired": "true"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
